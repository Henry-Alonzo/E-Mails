    <mj-raw>
      <!-- Socials -->
    </mj-raw>

    <mj-section padding="10px" background-color="white">
      <mj-column>
        <mj-social padding="0" mode="horizontal" align="center">
          <mj-social-element name="twitter" href="https://twitter.com/your-twitter-url" background-color="black" color="#FFFFFF" />
          <mj-social-element name="facebook" href="https://facebook.com/your-facebook-url" background-color="black" color="#FFFFFF" />
          <mj-social-element name="pinterest" href="https://pinterest.com/your-pinterest-url" background-color="black" color="#FFFFFF" />
          <mj-social-element name="instagram" href="https://instagram.com/your-instagram-url" background-color="black" color="#FFFFFF" />
        </mj-social>
      </mj-column>
    </mj-section>

    <mj-raw>
      <!-- Footer -->
    </mj-raw>

    <mj-section padding="20px" background-color="#f2f2f2">
      <mj-column>
        <mj-text align="center" font-size="10px" color="#777777" padding="0 0 10px 0" line-height="12px">
          You received this message because you previously expressed interest in news and offers from <a href="https://Vans.com" style="color: #0077CC; text-decoration: underline;">Vans.com</a> by supplying this email address. Vans respects your privacy and does not share your personal information with third parties. To <a href="https://unsubscribe-link.com" style="color: grey; text-decoration: underline;">UNSUBSCRIBE</a> from future email correspondence select unsubscribe.
        </mj-text>
        <mj-text align="center" font-size="10px" color="#777777" padding="0 0 10px 0" line-height="12px">
          To <a href="https://update-link.com" style="color: grey; text-decoration: underline;">UPDATE</a> your profile, select update.
        </mj-text>
        <mj-text align="center" font-size="10px" color="#777777" padding="0 0 10px 0" line-height="12px">
          This email was sent by: Vans, Inc.
        </mj-text>
        <mj-text align="center" font-size="10px" color="#777777" padding="0 0 10px 0" line-height="12px">
          1588 South Coast Drive Costa Mesa, CA, 92626, US
        </mj-text>
      </mj-column>
    </mj-section>

















