<mjml>
  <mj-head>
    <mj-attributes>
      <mj-image padding="0" />
      <mj-text padding="0" />
      <mj-all font-family="Helvetica,Arial" />
    </mj-attributes>
    <mj-style>
      * {
        box-sizing: border-box;
      }

      @media (max-width: 480px) {
        .mobile-links {
          padding: 0px !important;
        }
      }
    </mj-style>
    <mj-style inline="inline">

    </mj-style>
  </mj-head>

