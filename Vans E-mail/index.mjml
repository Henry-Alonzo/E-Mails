      <!-- Logo+ Nav Bar -->
    </mj-raw>

    <mj-section padding="10px 0 0 0" background-color="white">
      <mj-column width="25%">
        <mj-image href="https://Vans.com/blog" width="150px" padding-bottom="10px" src="assets/Logo.jpg" />
      </mj-column>

      <mj-column width="60%">
        <mj-text align="center" font-family="Helvetica, Arial" font-size="14px" color="#333333" padding="30px 0px 0px 0px" css-class="mobile-links">
